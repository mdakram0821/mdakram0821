// Function the name with no return type and argument
#include <stdio.h>

void add(int x,int y)
{
    int sum;
    sum=x+y;
    printf("sum%d",sum);
    
}
void main()
{
    int a,b;
    printf("Enter the value of a:");
    scanf("%d",&a);
    printf("Enter the value of b:");
    scanf("%d",&b);
    add(a,b);
}
